<?xml version='1.0' encoding='UTF-8'?>  
<!DOCTYPE hibernate-mapping PUBLIC  
 "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
 "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd"> 
 
<hibernate-mapping package="main.java.databasetables">
  <class name="TRatings" table="tratings">
    <id column="rating_id" name="rating_id" type="java.lang.Long">
      <generator class="increment"/>
    </id>
    <property column="user_id" name="user_id" type="java.lang.String"/>
    <property column="artist_id" name="artist_id" type="java.lang.String"/>
    <property column="rating" name="rating" type="java.lang.Integer"/>


	<many-to-one name="user" class="main.java.databasetables.TUsers" fetch="select">
            <column name="user_id" not-null="true" />
        </many-to-one>
        
    <many-to-one name="artist" class="main.java.databasetables.TArtists" fetch="select">
            <column name="artist_id" not-null="true" />
        </many-to-one>
  </class>
</hibernate-mapping>